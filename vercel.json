		},
		{
			"source": "/cigarettes/:path*",
			"destination": "http://api.smokehabit.com/cigarettes/:path*"
		},
		{
			"source": "/analytics/:path*",
			"destination": "http://api.smokehabit.com/analytics/:path*"
		},
		{
			"source": "/me",
			"destination": "http://api.smokehabit.com/me"
		},
		{
			"source": "/me/:path*",
			"destination": "http://api.smokehabit.com/me/:path*"
		}
	]
