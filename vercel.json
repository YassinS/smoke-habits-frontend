{
	"rewrites": [
		{
			"source": "/auth/:path*",
			"destination": "https://api.smoke-habits.yassinsassi.com/auth/:path*"
		},
		{
			"source": "/context",
			"destination": "https://api.smoke-habits.yassinsassi.com/context"
		},
		{
			"source": "/context/(.*)",
			"destination": "https://api.smoke-habits.yassinsassi.com/context/$1"
		},
		{
			"source": "/cigarettes",
			"destination": "https://api.smoke-habits.yassinsassi.com/cigarettes"
		},
		{
			"source": "/cigarettes/:path*",
			"destination": "https://api.smoke-habits.yassinsassi.com/cigarettes/:path*"
		},
		{
			"source": "/analytics/:path*",
			"destination": "https://api.smoke-habits.yassinsassi.com/analytics/:path*"
		},
		{
			"source": "/me",
			"destination": "https://api.smoke-habits.yassinsassi.com/me"
		},
		{
			"source": "/me/:path",
			"destination": "https://api.smoke-habits.yassinsassi.com/me/:path*"
		}
	]
}
